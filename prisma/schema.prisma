generator client {
  provider = "prisma-client-js"
  //output   = "../app/generated/prisma" // 指定しない
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Notes {
  id        String   @id @default(uuid())
  title     String
  content   String
  createdAt DateTime @default(now())

  @@map("notes")
}